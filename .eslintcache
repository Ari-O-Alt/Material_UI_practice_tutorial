TableContainer,\r\n    MyTableHead,\r\n    MyTablePagination,\r\n    recordsAfterPagingAndSorting,\r\n  } = useTable(records, headCells, filterFunction);\r\n\r\n  /**\r\n   *\r\n   * function that will handle the search action\r\n   */\r\n  const handleSearch = (event) => {\r\n    let target = event.target;\r\n    setFilterFunction({\r\n      // we set the state to a function that will return all the items if the search box is empty or the items that match the value in the search bar\r\n      fn(items) {\r\n        if (target.value === ' ') {\r\n          return items;\r\n        } else {\r\n          return items.filter((item) =>\r\n            item.fullName.toLowerCase().includes(target.value)\r\n          );\r\n        }\r\n      },\r\n    });\r\n  };\r\n\r\n  return (\r\n    <React.Fragment>\r\n      <PageHeader\r\n        icon={<PeopleIcon fontSize={'large'} />}\r\n        title={'New employee'}\r\n        subtitle={'Form design validation'}\r\n      />\r\n      <Paper className={classes.pageContent}>\r\n        <Toolbar>\r\n          <MyInput\r\n            className={classes.searchInput}\r\n            label={'Search employees'}\r\n            InputProps={{\r\n              startAdornment: (\r\n                <InputAdornment position={'start'}>\r\n                  <Search />\r\n                </InputAdornment>\r\n              ),\r\n            }}\r\n            onChange={handleSearch}\r\n          />\r\n          <MyButton\r\n            text={'Add New'}\r\n            variant='outlined'\r\n            startIcon={<AddIcon />}\r\n            className={classes.addNewButton}\r\n            onClick={() => setIsPopupOpen(true)}\r\n          />\r\n        </Toolbar>\r\n        <MyTableContainer>\r\n          <MyTableHead />\r\n          <TableBody>\r\n            {recordsAfterPagingAndSorting().map((record, index) => {\r\n              return (\r\n                <TableRow key={index}>\r\n                  <TableCell>{record.fullName}</TableCell>\r\n                  <TableCell>{record.city}</TableCell>\r\n                  <TableCell>{record.department}</TableCell>\r\n                  <TableCell>{record.mobile}</TableCell>\r\n                </TableRow>\r\n              );\r\n            })}\r\n          </TableBody>\r\n        </MyTableContainer>\r\n        <MyTablePagination />\r\n      </Paper>\r\n      <MyPopup\r\n        isPopupOpen={isPopupOpen}\r\n        title={'Employee form'}\r\n        setIsPopupOpen={setIsPopupOpen}\r\n      >\r\n        <EmployeeForm />\r\n      </MyPopup>\r\n    </React.Fragment>\r\n  );\r\n};\r\n\r\nexport default Employees;\r\n","D:\\+proiecte+programare+\\react_MUI_youtube_tutorial\\mui_react_app\\Material_UI_practice_tutorial\\src\\Components\\SideMenu\\SideMenu.jsx",[],"D:\\+proiecte+programare+\\react_MUI_youtube_tutorial\\mui_react_app\\Material_UI_practice_tutorial\\src\\Components\\Header\\Header.jsx",[],"D:\\+proiecte+programare+\\react_MUI_youtube_tutorial\\mui_react_app\\Material_UI_practice_tutorial\\src\\Pages\\Employees\\EmployeeForm.js",[],"D:\\+proiecte+programare+\\react_MUI_youtube_tutorial\\mui_react_app\\Material_UI_practice_tutorial\\src\\Components\\useTable.js",[],"D:\\+proiecte+programare+\\react_MUI_youtube_tutorial\\mui_react_app\\Material_UI_practice_tutorial\\src\\Services\\employeeService.js",[],"D:\\+proiecte+programare+\\react_MUI_youtube_tutorial\\mui_react_app\\Material_UI_practice_tutorial\\src\\Components\\Controls\\MyInput.js",[],"D:\\+proiecte+programare+\\react_MUI_youtube_tutorial\\mui_react_app\\Material_UI_practice_tutorial\\src\\Components\\PageHeader\\PageHeader.jsx",[],"D:\\+proiecte+programare+\\react_MUI_youtube_tutorial\\mui_react_app\\Material_UI_practice_tutorial\\src\\Components\\useForm.js",[],"D:\\+proiecte+programare+\\react_MUI_youtube_tutorial\\mui_react_app\\Material_UI_practice_tutorial\\src\\Components\\Controls\\MyRadioGroup.js",[],"D:\\+proiecte+programare+\\react_MUI_youtube_tutorial\\mui_react_app\\Material_UI_practice_tutoria